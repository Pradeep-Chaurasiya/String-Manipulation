public class Solution {
    public int solve(char[] A) {
        
        for(int i=0; i<A.length; i++)
        {
            if(!((A[i]>='a' && A[i]<='z') || (A[i]>='A' && A[i]<='Z') || (A[i]>='0' && A[i]<='9')))
            {
                return 0;
            }
        }
        return 1;
    }
}
