public class Solution {
    public String solve(String A, int B) {
        
        int N = A.length();
        B = B%N;
        StringBuilder sb = new StringBuilder();
        
        for(int i=N-B; i<=N-1; i++)
        {
            sb.append(A.charAt(i));
        }
        
         for(int i=0; i<N-B; i++)
        {
            sb.append(A.charAt(i));
        }
        
        return sb.toString();
        
        
    }
}
